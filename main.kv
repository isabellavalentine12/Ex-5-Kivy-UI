<MainScreen>:
    #: import time __main__.time

    name: 'main'
    #I added the below command while creating my counter
    counter_button: counter_button #right side references id. Left side is refencered in python file.
    FloatLayout:
        size_hint: None, None

    DPEAButton:
        id: test_button
        text: 'Off' if test_button.state == 'normal' else 'On' #normal must be lowercase. Single or double quotes don't matter, as long as they're consistent on a word
        size: 100, 100
        x: root.width * 0.75
        y: root.height * 0.5
        color: 0, .5, .5, 1
        on_press: root.pressed()

    DPEAButton:
        text: "test2"
        size: 100, 100
        x: root.width * 0.25
        y: root.height * 0.5
        color: .9, 0, 0, 1
        on_press: root.pressed()

#I added the following button to create my button
    DPEAButton:
        id: counter_button
        text: "0"
        size: 100, 100
        x: root.width * 0.5
        y: root.height * 0.15
        color: .8, 0, .5, 1
        on_press: root.pressed()

    SelfUpdatingLabel:
        id: time_label
        size_hint: None, None
        font_size: 30
        update_property: time.now
        update_property_parameters: None
        center_x: root.width * 0.5
        center_y: root.height * 0.45
        color: 1, 1, 1, 1
        bold: True
        outline_width: self.font_size * 0.1
        outline_color: 0, 0, 0